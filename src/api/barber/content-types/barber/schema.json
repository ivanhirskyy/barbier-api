{
  "kind": "collectionType",
  "collectionName": "barbers",
  "info": {
    "singularName": "barber",
    "pluralName": "barbers",
    "displayName": "Barber",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "avatar": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "phone": {
      "type": "string",
      "minLength": 9
    },
    "appointments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::appointment.appointment",
      "mappedBy": "barber"
    },
    "uid": {
      "type": "customField",
      "customField": "plugin::strapi-advanced-uuid.uuid"
    }
  }
}
